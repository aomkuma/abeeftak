	<subDataset name="Table Dataset 1"/>
	<parameter name="CtrlId" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
t0.referenceNumber
,t0.ReceivedControlNumber
,t0.transhipmentApplicantTaxNumber
,t0.transhipmentApplicantCompanyThaiName
,t3.street
,t3.district
,t3.subProvince
,t3.province
,t3.postcode
,t2.vesselNameImport
,IF(t2.arrivalDateImport = '0000-00-00 00:00:00' , NULL, t2.arrivalDateImport) AS arrivalDateImport
,t0.containerNumber
,t0.containerSizeandTypeCode
,t2.dischargePortImport
,t5.portCode AS dischargePortCodeImport
,t5.portname AS dischargePortNameImport
,t2.loadPortExport
,t6.portCode AS dischargePortCodeExport
,t6.portname AS dischargePortNameExport
,t2.shedNumberReleasePortExport
,t4.portName AS portNameExport
,t1.transhipmentTransitDeclaration
,GROUP_CONCAT(DISTINCT(t2.houseBillofLadingImport) SEPARATOR ', ') AS groupBL
-- ,SUM(t2.packageAmountImport) AS sumPackageAmount
,(
SELECT SUM(tbl0.packageAmountImport)
FROM tcmbl tbl0
WHERE HID = t0.CtrlId AND transId = t1.TransSecId

)  AS sumPackageAmount
,(
		SELECT
			SUM(
				IF(tbl1.unit2 IS NOT NULL
				,tbl0.grossWeightImport / tbl1.constval
				,tbl0.grossWeightImport)
			) AS calc_gw
		FROM tcmbl tbl0
		LEFT JOIN master_unit_converter tbl1
		ON (tbl0.grossWeightUnitCodeImport = tbl1.unit1 AND tbl1.unit2 = 'KGM')
		WHERE HID = t0.CtrlId AND transId = t1.TransSecId
) AS sumGW
,t0.modeOfMovement
,t0.carLicense
,t0.GPSID
,t0.trailerLicense
,COUNT(DISTINCT t2.houseBillofLadingImport) AS cnt_hawb
FROM tcmcontrol t0
INNER JOIN tcmnumber t1
ON t0.CtrlId = t1.HID
INNER JOIN tcmbl t2
ON t1.TransSecId = t2.transId
LEFT JOIN conf_customer t3
ON (t0.transhipmentApplicantTaxNumber = t3.tax AND t0.transhipmentApplicantCompanyBranch = t3.branch AND modeOtherParty = 'checked')
LEFT JOIN master_port t4
ON t2.shedNumberReleasePortExport = t4.portCode
LEFT JOIN master_port t5
ON t2.dischargePortImport = t5.portCode
LEFT JOIN master_port t6
ON t2.loadPortExport = t6.portCode
WHERE t0.CtrlId = $P{CtrlId}
GROUP BY t1.transhipmentTransitDeclaration]]>
	</queryString>
