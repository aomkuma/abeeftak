	<subDataset name="Table Dataset 1"/>
	<queryString>
		<![CDATA[SELECT
     *,
     users.`id` AS users_id,
     users.`title` AS users_title,
     users.`firstname` AS users_firstname,
     users.`lastname` AS users_lastname,
     users.`email` AS users_email,
     users.`password` AS users_password,
     users.`role_id` AS users_role_id,
     users.`position` AS users_position,
     users.`created` AS users_created,
     users.`createdby` AS users_createdby,
     users.`updated` AS users_updated,
     users.`updatedby` AS users_updatedby
FROM
     `users` users
INNER JOIN roles ON users.role_id=roles.id
]]>
	</queryString>
